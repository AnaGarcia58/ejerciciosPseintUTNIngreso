Algoritmo RestoDivision
	Definir num1, num2 Como Entero;
	Definir resto Como Entero;
	num1 = 0;
	num2 = 0;
	resto = 0;
	
	Escribir "Por favor, ingrese un número";
	Leer num1;
	Escribir "Por favor, ingrese un número";
	Leer num2;
	
	resto = num1 mod num2;
	
	Escribir "El resto de la división entre ",num1, " y ",num2, " es ", resto; 
	
	
	
	
FinAlgoritmo
